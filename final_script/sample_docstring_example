def swap_add(a=5,b=10):
	/*
	Summary of swap_add { Function Swap variable and Return their Sum }
	
	Keyword Arguments: 
		a {int} -- {[description]} (default: {5})
		b {int} -- {[description]} (default: {10})
	
	
	Temporary variables (VarName,Type) : {[description]}
	
	Return: 
		int -- {[description]}
	
	*/
	
	temp = a
	a = b
	b = temp
	
	temp = a+b
	
	return temp
	

swap_add(10,5)
